@model Transportmk2.Models.Storage
@{
    ViewBag.Title = "Удаление склада";
}

<div>
    <dl class="dl-horizontal">
        <dt>Id</dt>
        <dd>
            @Html.DisplayFor(model => model.Storage_id)
        </dd>

        <dt>Название</dt>
        <dd>
            @Html.DisplayFor(model => model.Storage_name)
        </dd>
        <dt>Широта</dt>
        <dd>
            @Html.DisplayFor(model => model.Latitude)
        </dd>
        <dt>Долгота</dt>
        <dd>
            @Html.DisplayFor(model => model.Longitude)
        </dd>
        <dt>Товара в наличии</dt>
        <dd>
            @Html.DisplayFor(model => model.Capacity)
        </dd>
        <dt>
    </dl>

    <div>
        <form asp-controller="Home" asp-action="Delete" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Удалить" />
            </div>
        </form>
    </div>
</div>